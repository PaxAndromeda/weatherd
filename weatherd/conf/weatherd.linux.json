{
  "DataSource": "Test",
  "Site": {
    "Elevation": 113
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Serilog": {
    "Using": [ "AWS.Logger.SeriLog" ],
    "Region": "us-east-1",
    "LogGroup": "weatherd",
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "WeatherService": {
    "EnableCorrectness": true,
    "Elevation": 113
  },
  "TimestreamService": {
    "EnableDataWrite": false,
    "Database": "weather",
    "Table": "local",
    "Dimensions": [
      {
        "Name": "Location",
        "DimensionValueType": "VARCHAR",
        "Value": "Local"
      }
    ],
    "Records": [
      {
        "Name": "Temperature",
        "Property": "Temperature",
        "Unit": "DegreesCelsius",
        "Type": "DOUBLE"
      },
      {
        "Name": "Dewpoint",
        "Property": "Dewpoint",
        "Unit": "DegreesCelsius",
        "Type": "DOUBLE"
      },
      {
        "Name": "RelativeHumidity",
        "Property": "RelativeHumidity",
        "Unit": "Percent",
        "Type": "DOUBLE"
      },
      {
        "Name": "BarometricPressure",
        "Property": "Pressure",
        "Unit": "Hectopascals",
        "Type": "DOUBLE"
      },
      {
        "Name": "SeaLevelPressure",
        "Property": "SeaLevelPressure",
        "Unit": "Hectopascals",
        "Type": "DOUBLE"
      },
      {
        "Name": "Luminosity",
        "Property": "Luminosity",
        "Unit": "WattsPerSquareMeter",
        "Type": "DOUBLE"
      },
      {
        "Name": "WindSpeed",
        "Property": "WindSpeed",
        "Unit": "MetersPerSecond",
        "Type": "DOUBLE"
      },
      {
        "Name": "WindDirection",
        "Property": "WindDirection",
        "Unit": "Degrees",
        "Type": "DOUBLE"
      },
      {
        "Name": "RainSinceMidnight",
        "Property": "RainSinceMidnight",
        "Unit": "Millimeters",
        "Type": "DOUBLE"
      },
      {
        "Name": "SnowSinceMidnight",
        "Property": "SnowfallSinceMidnight",
        "Unit": "Millimeters",
        "Type": "DOUBLE"
      },
      {
        "Name": "Visibility",
        "Property": "Visibility",
        "Unit": "Meters",
        "Type": "DOUBLE"
      },
      {
        "Name": "Weather",
        "Property": "Weather",
        "Type": "VARCHAR"
      },
      {
        "Name": "Weather15M",
        "Property": "WeatherLast15Minutes",
        "Type": "VARCHAR"
      },
      {
        "Name": "Weather1H",
        "Property": "WeatherLastHour",
        "Type": "VARCHAR"
      },
      {
        "Name": "PWDRainfallSinceMidnight",
        "Property": "OpticalRainfallSinceMidnight",
        "Unit": "Millimeters",
        "Type": "DOUBLE"
      },
      {
        "Name": "PWDWaterIntensity",
        "Property": "WaterIntensity",
        "Unit": "MillimetersPerHour",
        "Type": "DOUBLE"
      },
      {
        "Name": "BatteryChargeCurrent",
        "Property": "BatteryChargeCurrent",
        "Unit": "Ampere",
        "Type": "DOUBLE"
      },
      {
        "Name": "BatteryDrainCurrent",
        "Property": "BatteryDrainCurrent",
        "Unit": "Ampere",
        "Type": "DOUBLE"
      },
      {
        "Name": "BatteryVoltage",
        "Property": "BatteryVoltage",
        "Unit": "VoltDc",
        "Type": "DOUBLE"
      }
    ]
  },
  "PakbusDataSource": {
    "Port": {
      "Name": "/dev/ttySC1",
      "Baud": 9600
    },
    "NodeID": 4092,
    "TargetNode": 1,
    "SecurityCode": 0
  },
  "PWD12DataSource": {
    "Port": {
      "Name": "/dev/ttySC0",
      "Baud": 9600
    }
  },
  "TestDataSource": {
    "Enabled": true,
    "PollingInterval": 1,
    "SampleInterval": 1,

    "Latitude": 47.5,
    "Longitude": -122.25,

    "Synthetics": {
      "Temperature": {
        "Type": "Diurnal",
        "Min": 15,
        "Max": 30,
        "Trough": 4,
        "Period": 24,
        "Deviation": 0.2
      },
      "Dewpoint": {
        "Type": "Diurnal",
        "Min": 10,
        "Max": 25,
        "Trough": 4,
        "Period": 24,
        "Deviation": 0.2
      },
      "Pressure": {
        "Type": "Diurnal",
        "Min": "1013.25",
        "Max": "1014.65",
        "Trough": 4,
        "Period": 12,
        "Deviation": 0.1
      },
      "Wind": {
        "Type": "SyntheticWind",
        "PeakWindSpeedHour": 15,
        "DiurnalPatternStrength": 0.25,
        "OneHourAutocorrelationFactor": 0.85,
        "WeibullShapeFactor": 2.0,
        "MeanWindSpeed": 3.93,
        "TimeStep": 1
      }
    }
  }
}